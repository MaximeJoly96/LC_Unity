<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<EventsSequence>
	<ControlTimer Key="timer1">
		<Action>Start</Action>
		<Duration>10</Duration>
	</ControlTimer>
	<ControlTimer Key="timer2">
		<Action>Start</Action>
		<Duration>40</Duration>
	</ControlTimer>
	
	<ConditionalBranch Type="TimerCondition" Condition="Before" FirstMember="timer1" SecondMember="20" />
	<ConditionalBranch Type="TimerCondition" Condition="After" FirstMember="timer1" SecondMember="timer2" />
	<ConditionalBranch Type="TimerCondition" Condition="After" FirstMember="20" SecondMember="timer2" />
  
</EventsSequence>